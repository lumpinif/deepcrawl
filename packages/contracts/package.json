{
  "name": "@deepcrawl/contracts",
  "version": "0.0.1",
  "description": "Shared API contracts for DeepCrawl services",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md"
