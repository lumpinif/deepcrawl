{
  "name": "deepcrawl",
  "version": "0.2.5",
  "description": "JavaScript/TypeScript SDK for DeepCrawl API - A powerful web scraping and crawling service",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "check": "biome check --write && pnpm typecheck",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run check && pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "deepcrawl",
    "web-scraping",
    "crawling",
    "api",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "Felix Lu(Lyu) <@felixlu1018>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumpinif/deepcrawl",
    "directory": "packages/sdks/js-ts"
  },
  "bugs": {
    "url": "https://github.com/lumpinif/deepcrawl/issues"
  },
  "homepage": "https://github.com/lumpinif/deepcrawl#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.12.1",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@deepcrawl/contracts": "workspace:*",
    "@deepcrawl/types": "workspace:*",
    "@deepcrawl/typescript-config": "workspace:*",
    "@orpc/client": "^1.7.4",
    "@orpc/contract": "^1.7.4",
    "@types/node": "^24.1.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "msw": "^2.10.3",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
