{
  "name": "deepcrawl",
  "version": "0.5.4",
  "description": "JavaScript/TypeScript SDK for Deepcrawl API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types-public.d.ts",
        "default": "./dist/types-public.js"
      },
      "require": {
        "types": "./dist/types-public.d.cts",
        "default": "./dist/types-public.cjs"
      }
    },
    "./types/utils": {
      "import": {
        "types": "./dist/types-utils.d.ts",
        "default": "./dist/types-utils.js"
      },
      "require": {
        "types": "./dist/types-utils.d.cts",
        "default": "./dist/types-utils.cjs"
      }
    },
    "./zod/v4": {
      "import": {
        "types": "./dist/zod-v4.d.ts",
        "default": "./dist/zod-v4.js"
      },
      "require": {
        "types": "./dist/zod-v4.d.cts",
        "default": "./dist/zod-v4.cjs"
      }
    },
    "./schemas": {
      "import": {
        "types": "./dist/schemas-public.d.ts",
        "default": "./dist/schemas-public.js"
      },
      "require": {
        "types": "./dist/schemas-public.d.cts",
        "default": "./dist/schemas-public.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check --write && pnpm typecheck",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run check && pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "deepcrawl",
    "web-scraping",
    "crawling",
    "api",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": {
    "name": "Felix Lu",
    "url": "https://x.com/felixlu1018"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumpinif/deepcrawl",
    "directory": "packages/sdks/js-ts"
  },
  "bugs": {
    "url": "https://github.com/lumpinif/deepcrawl/issues"
  },
  "homepage": "https://github.com/lumpinif/deepcrawl#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@deepcrawl/contracts": "workspace:*",
    "@deepcrawl/types": "workspace:*",
    "@deepcrawl/typescript-config": "workspace:*",
    "@orpc/client": "catalog:",
    "@orpc/contract": "catalog:",
    "@types/node": "catalog:",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "msw": "^2.12.7",
    "tsdown": "^0.15.12",
    "typescript": "catalog:",
    "vitest": "^3.2.4"
  }
}
