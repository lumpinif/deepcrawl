    "better-auth": "^1.3.9",
    "dotenv-cli": "^10.0.0",
    "hono": "^4.9.6",
    "husky": "^9.1.7",
    "markdownlint-cli2": "^0.18.1",
    "rimraf": "^6.0.1",
    "shadcn": "^3.2.1",
