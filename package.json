{
  "name": "deepcrawl",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format && biome format --write .",
    "lint:repo": "pnpm dlx sherif@latest",
    "lint:repo:fix": "pnpm dlx sherif@latest --fix",
    "fix": "turbo lint && turbo fix && pnpm format && turbo typecheck && pnpm lint:repo:fix",
    "db:gen-types": "pnpm dlx supabase gen types typescript --project-id $PROJECT_REF --schema public > supabase/types/database.types.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@deepcrawl/eslint-config": "workspace:*",
    "@deepcrawl/typescript-config": "workspace:*",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "dotenv-cli": "^8.0.0",
    "supabase": "^2.23.4",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": ">=20"
  }
}
